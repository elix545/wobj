SUBROUTINE WOBJ.SET(NODE,VALUE,RERR)
*
INCLUDE WOBJ.INCLUDE
*
IF WOBJ.DEBUG THEN PRINT "WOBJ.SET: NODE=":NODE
CALL WOBJ.FINDNODE(NODE,NODEPOS,NODEVALUE,LASTNODE,RERR)
IF WOBJ.DEBUG THEN PRINT "WOBJ.SET: NODEPOS=":NODEPOS:" LASTNODE=":LASTNODE:" PARTPOS=":NODEPOS:" RERROR=":RERR<2>
*
IF NODE AND NOT(NODEPOS) THEN

END
IF NODE AND NODEPOS THEN
	* WE FOUND A POSITION
	IF NUM(PARTVALUE) THEN PARTTYPE="D" ELSE PARTTYPE="S"
	CALL WOBJ.SET.PART(NODE,NODEPOS,PARTTYPE,VALUE,RERR)
END
RETURN
