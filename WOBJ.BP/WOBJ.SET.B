SUBROUTINE WOBJ.SET(NODE,VALUE,RERR)
*
INCLUDE WOBJ.INCLUDE
*
IF WOBJ.DEBUG THEN PRINT "WOBJ.SET: NODE=":NODE
CALL WOBJ.FINDNODE(NODE,NODEPOS,PARTPOS,RERR)
IF WOBJ.DEBUG THEN PRINT "WOBJ.SET: NODEPOS=":NODEPOS:" PARTPOS=":PARTPOS:" RERROR=":RERR<2>
*
IF NODEPOS AND PARTPOS THEN
	* WE FOUND A POSITION
	IF NUM(PARTVALUE) THEN PARTTYPE="D" ELSE PARTTYPE="S"
	CALL WOBJ.SET.PART(NODEPOS,PARTPOS,PARTTYPE,VALUE,RERR)
END
RETURN
