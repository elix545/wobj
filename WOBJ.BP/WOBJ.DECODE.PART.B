SUBROUTINE WOBJ.DECODE.PART(NODEPOS,PARTPOS,VALUE)
*
INCLUDE WOBJ.INCLUDE
*
PARTTYPE		=	JSONOBJECT(JSONOBJECT$NODEPARTS.TYPE)<1,NODEPOS,PARTPOS>
PARTENCODING	=	JSONOBJECT(JSONOBJECT$NODEPARTS.ENCODING)<1,NODEPOS,PARTPOS>
PARTVALUE		=	JSONOBJECT(JSONOBJECT$NODEPARTS.VALUE)<1,NODEPOS,PARTPOS>
*
BEGIN CASE
	CASE PARTENCODING	=	"H"
		VALUE=OCONV(PARTVALUE,"MY")
	CASE 1
		VALUE=PARTVALUE
END CASE
*
RETURN
