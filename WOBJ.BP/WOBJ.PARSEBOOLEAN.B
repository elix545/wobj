SUBROUTINE WOBJ.PARSEBOOLEAN(JSTR,INDEXPOS,PARSESTRING)
*
INCLUDE WOBJ.INCLUDE
*
PARSESTRING=""
*
CALL WOBJ.SKIPCHAR(JSTR,INDEXPOS)
BEGIN CASE
   CASE JSTR[INDEXPOS,4] = "true"
	  PARSESTRING="true"
	  INDEXPOS+=4
   CASE JSTR[INDEXPOS,5] = "false"
      PARSESTRING="false"
      INDEXPOS+=5
   CASE 1
   	  PSERRORS<-1>="Invalid Boolean at position ":INDEXPOS:" : ":JSTR[INDEXPOS,5]
   	  RETURN
END CASE
RETURN
